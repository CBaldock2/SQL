/* Selects all from the WorkoukMuscles table */
SELECT * FROM WorkoutMuscles


/* Lists the workout, muscle, exercise, how to do the exercise, what the primary function is for calf workouts */
SELECT w.WorkoutName, m.MuscleName, e.ExerciseName, e.Technique, m.PrimaryFunction, wm.MuscleRole
FROM WorkoutMuscles wm
JOIN muscles m on wm.MuscleID=m.MuscleID
JOIN workouts w on wm.WorkoutID=w.WorkoutID
JOIN exercises e on wm.ExerciseID=e.ExerciseID
WHERE m.MuscleGroup = 'Calf'
ORDER BY w.WorkoutName;

/* Lists all the exercises in the chest and tricep workout */
SELECT w.WorkoutName, e.ExerciseName, m.MuscleName, wm.MuscleRole
FROM Workouts w
JOIN WorkoutMuscles wm ON w.WorkoutID = wm.WorkoutID
JOIN Exercises e ON wm.ExerciseID = e.ExerciseID
JOIN Muscles m ON wm.MuscleID = m.MuscleID
WHERE w.WorkoutName = 'Chest and Tricep'
ORDER BY wm.MuscleRole;

/* This query lists each workout along with the primary muscles targeted */
SELECT w.WorkoutName, m.MuscleName
FROM Workouts w
JOIN WorkoutMuscles wm ON w.WorkoutID = wm.WorkoutID
JOIN Muscles m ON wm.MuscleID = m.MuscleID
WHERE wm.MuscleRole = 'Primary'
ORDER BY w.WorkoutName;

/* Counts how many exercises per workout */
SELECT w.WorkoutName, COUNT(e.ExerciseID) AS NumberOfExercises
FROM Workouts w
JOIN WorkoutMuscles wm ON w.WorkoutID = wm.WorkoutID
JOIN Exercises e ON wm.ExerciseID = e.ExerciseID
GROUP BY w.WorkoutName
ORDER BY NumberOfExercises DESC;

/* Lists detailed information about each exercise within a workout, including descriptions and techniques */
SELECT w.WorkoutName, e.ExerciseName, e.Description, e.Technique
FROM Workouts w
JOIN WorkoutMuscles wm ON w.WorkoutID = wm.WorkoutID
JOIN Exercises e ON wm.ExerciseID = e.ExerciseID
WHERE w.WorkoutName = 'Leg Day (Front)';

/* List all muscles used in each workout with the exercise and wether it is a primary or secondary exercise */
SELECT w.WorkoutName, m.MuscleName, e.ExerciseName, wm.MuscleRole
FROM Workouts w
JOIN WorkoutMuscles wm ON w.WorkoutID = wm.WorkoutID
JOIN Exercises e ON wm.ExerciseID = e.ExerciseID
JOIN Muscles m ON wm.MuscleID = m.MuscleID
ORDER BY w.WorkoutName, wm.MuscleRole;

/* Uses a CTE to select the first exercise from each workout*/ 
WITH RankedExercises AS (
    SELECT w.WorkoutName, e.ExerciseName, RANK() OVER (PARTITION BY w.WorkoutName ORDER BY e.ExerciseID) AS Rank
    FROM Workouts w
    JOIN WorkoutMuscles wm ON w.WorkoutID = wm.WorkoutID
    JOIN Exercises e ON wm.ExerciseID = e.ExerciseID
)
SELECT WorkoutName, ExerciseName
FROM RankedExercises
WHERE Rank = 1;
