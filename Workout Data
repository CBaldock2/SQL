/* Creates a table for information about muscles */
CREATE TABLE muscles (
	MuscleID INT IDENTITY(1,1) PRIMARY KEY,
	MuscleName VARCHAR(255),
	MuscleGroup VARCHAR(255),
	PrimaryFunction VARCHAR(255)
);

/* Adds a Location column */
ALTER TABLE muscles
ADD Location VARCHAR(255);

/* Inserts values into the table */
